{
  "name": "lipsurf-plugins",
  "version": "2.0.0",
  "description": "Plugins for the LipSurf Chrome extension. Each plugin adds a set of commands to LipSurf.",
  "scripts": {
    "docs:watch": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "build": "lipsurf-cli build",
    "watch": "lipsurf-cli build --watch",
    "vup": "lipsurf-cli vup",
    "build:prod": "cross-env NODE_ENV=production lipsurf-cli build",
    "watch:prod": "cross-env NODE_ENV=production lipsurf-cli build --watch",
    "clean": "rimraf -r dist/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lipsurf/plugins"
  },
  "keywords": [
    "voice",
    "recognition",
    "hands-free",
    "abilities",
    "accessibility"
  ],
  "engines": {
    "node": ">=11"
  },
  "author": "Miko",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/lipsurf/plugins/issues"
  },
  "homepage": "https://www.lipsurf.com",
  "devDependencies": {
    "@types/chrome": "0.0.66",
    "cross-env": "^7.0.2",
    "lipsurf-cli": "https://github.com/lipsurf/cli",
    "lipsurf-types": "https://github.com/lipsurf/types",
    "rimraf": "^3.0.2",
    "typescript": "^4.0.2",
    "vuepress": "^1.6.0"
  }
}
